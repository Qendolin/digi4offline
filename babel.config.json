{
	"plugins": ["@babel/plugin-transform-modules-commonjs", "transform-node-env-inline"],
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "18"
				}
			}
		],
		[
			"minify",
			{
				"keepClassName": false,
				"keepFnName": false,
				"tdz": false
			}
		]
	],
	"comments": false,
	"only": ["./main.js", "index.js", "./src/"]
}
