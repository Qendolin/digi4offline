{
	"name": "digi4offline",
	"version": "1.1.5",
	"description": "Node.js based command line tool for downloading digi4school books as PDFs for offline use.",
	"main": "index.js",
	"module": "main.js",
	"author": "Wendelin Muth",
	"license": "MPL-2.0",
	"type": "commonjs",
	"scripts": {
		"build": "cross-env NODE_ENV=production babel --delete-dir-on-start -d lib .",
		"prepublish": "npm run build",
		"postinstall": "patch-package"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Qendolin/digi4offline.git"
	},
	"bin": {
		"digi4offline": "./bin/digi4offline.js"
	},
	"keywords": [
		"digi4school",
		"download",
		"downloader",
		"offline",
		"pdf",
		"pdfkit"
	],
	"dependencies": {
		"canvas": "^2.9.1",
		"canvg": "^3.0.7",
		"commander": "^6.1.0",
		"esm": "^3.2.25",
		"jsdom": "^19.0.0",
		"jspdf": "^2.5.1",
		"memoizee": "^0.4.15",
		"node-fetch": "^2.6.1",
		"patch-package": "^6.4.7",
		"pdfkit": "^0.11.0",
		"read": "^1.0.7",
		"svg-to-pdfkit": "^0.1.8",
		"svg2pdf.js": "^2.1.0",
		"tiny-async-pool": "^1.1.0",
		"xmldom": "^0.5.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.13.14",
		"@babel/core": "^7.13.14",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-transform-modules-commonjs": "^7.13.8",
		"@babel/preset-env": "^7.13.15",
		"@types/node-fetch": "^2.5.10",
		"babel-plugin-transform-node-env-inline": "^0.4.3",
		"babel-preset-minify": "^0.5.1",
		"cross-env": "^7.0.3",
		"pkg": "^4.5.1"
	}
}
